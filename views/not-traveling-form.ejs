                                                    <div class="contact-form-box">
                                                        <form>
                                                            <div class="row">
                                                                <div class="col-md-10 m-auto text-center days-content">
                                                                    <p class="text-purple">
                                                                        Day 1:  Test On
                                                                    </p>
                                                                    <ul>
                                                                        <li>
                                                                            <p>
                                                                                <i class="far fa-calendar-day"></i>
                                                                               <%=date%>
                                                                            </p>
                                                                        </li>
                                                                   
                                                                    </ul>
                                                                </div>
                                                           
                                                            </div>
                                                            <div class="row mt-4">
                                                                <div class="col-12 text-center">
                                                                    <p class="mb-0">
                                                                        Your booking slot will be reserved for 30mins to allow you time to complete your booking.
                                                                    </p>
                                                                </div>
                                                            </div>
                                                        </form>
                                                    </div>
                                                  
                                                    <div class="price text-center mt-4 mb-4">
                                                        <!-- <p class="text-purple">
                                                            Price : 
                                                            <span><%=testDetails.base_price%></span>
                                                        </p> -->
                                                    </div>
<% people.map((people,index)=>{%>
                                                    <div class="contact-form-box">
                                                        <div class="text-center">

                                                            <h2>
                                                                Person 
                                                                <span class="text-skyblue">
                                                                    <%=index+1%> of <%= length%>
                                                                </span>
                                                            </h2>
                                                        </div>
                                                        <div class="row">
                                                            <div class="col-md-6 form-group">
                                                                <label>First name*</label>
                                                                <input type="text" name="fname"
                                                                 id="fname<%=index%>" class="w-100 field" placeholder="Enter your first name" required="">
                                                            </div>
                                                            <div class="col-md-6 form-group">
                                                                <label>Last name*</label>
                                                                <input type="text" name="lname" id="lname<%=index%>" class="w-100 field" placeholder="Enter your last name" required="">
                                                            </div>
                                                            <div class="col-md-6 form-group">
                                                                <label>DOB*</label>
                                                                <input type="date" name="dob" id="dob<%=index%>" class="w-100 field" placeholder="Your date of departure" required="">
                                                                <span class="text-lightpurple d-block mt-1">Required by Public Health Authorities</span>
                                                            </div>
                                                            <div class="col-md-6 form-group">
                                                                <label>Sex*</label>
                                                                <select id='sex<%=index%>' class="w-100 field">
                                                                    <option value="male">Male</option>
                                                                    <option value="female">Female</option>
                                                                </select>
                                                                <span class="text-lightpurple d-block mt-1">Required by Public Health Authorities</span>
                                                            </div>
                                                            <div class="col-md-12 form-group">
                                                                <label>Home Address*</label>
                                                                <input type="text" name="address" id="address<%=index%>" class="w-100 field" placeholder="Enter your address" required="">
                                                            </div>
                                                            <div class="col-md-6 form-group">
                                                                <label>Email*</label>
                                                                <input type="email" name="email" id="email<%=index%>" class="w-100 field" placeholder="Enter your email" required="">
                                                                <span class="text-lightpurple d-block mt-1">We will send your test details by both SMS/Email</span>
                                                            </div>
                                                           
                                                            <div class="col-md-6 form-group">
                                                                <label>Mobile number*</label>
                                                                <div class="row">
                                                                  
                                                                    <div class="col-lg-9 col-md-8 col-12">
                                                                        <input type="tel" name="number" id='phone<%=index%>'
                                                                        class="w-100 field"
                                                                         placeholder="Enter your phone" required="">
                                                                    </div>
                                                                </div>
                                                            </div>
                                                                                                             
                                                            <div class="col-12">
                                                                <hr class="mt-0 mb-4">
                                                            </div>
                                                         
                                                            <div class="col-md-6 form-group">
                                                                <label>COVID-19 Vaccination Status*</label>                                                                
                                                                <select id='vaccination_status<%=index%>' value="" class="w-100 field">
                                                                    <option value="">Select</option>
                                                                    <option value="pending">Pending</option>
                                                                    <option value="fully">fully</option>
                                                                    <option value="partially">partially</option>
                                                                </select>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <% })%>
                                                    <div class="col-md-6 text-md-right text-center">
                                                        <a onclick="getData('<%=length%>','<%=spots%>','<%=packageid%>')" class="btn-purple d-inline-block mt-4 mb-md-0 mb-3">
                                                            CONTINUE
                                                        </a>
                                                    </div>
                                            
                                            </div>
                                        </div>

                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </section>
