								<h1><%=data.firstName%></h1>
							
								
								<form id="loginForm" action="/update-personal-details/<%=id%>" method="POST">
									<div class="form-group">
										<label class="form-control-label">Email Address</label>
										<input type="email" class="form-control" id="email" name="email" value="<%=data.email%>">
									</div>
							
									<div class="form-group">
										<label class="form-control-label"> First Name </label>
										<div class="pass-group">
											<input type="text" name="firstName" 
											id="firstName" class="form-control pass-input" value="<%=data.firstName%>">
										</div>
									</div>
									<div class="form-group">
										<label class="form-control-label"> Last Name </label>
										<div class="pass-group">
											<input type="text" name="lastName" 
											id="lastName" class="form-control pass-input" value="<%=data.lastName%>">
										</div>
									</div>
									<div class="form-group">
										<label class="form-control-label"> Phone </label>
										<div class="pass-group">
											<input type="text" name="phone" 
											id="phone" class="form-control pass-input" value="<%=data.phone%>">
										</div>
									</div>
									<div class="form-group">
										<label class="form-control-label"> Date Of Birth </label>
										<div class="pass-group">
											<input type="date" name="dob" 
											id="dob" class="form-control pass-input" value="<%=data.dob%>">
										</div>
									</div>
									<div class="form-group">
										<label class="form-control-label"> Address </label>
										<div class="pass-group">
											<input type="text" name="address" 
											id="address" class="form-control pass-input" value="<%=data.address%>">
										</div>
									</div>
									<div class="form-group">
										<label class="form-control-label"> Gender </label>
									
									<select name='gender' value='<%=data.gender%>'
									 id='gender' class="form-control pass-input"
									  >
										<option value='male'>male</option>
										<option value='female'>female</option>
										<option value='others'>others</option>
									  </select>
                                      <div class="contact-form-box">
                                        <div class="row">
                                            <div class="col-12 text-center">
                                                <h4 class="text-purple mb-5">
                                                    Additional information required
                                                </h4>
                                            </div>
                                        </div>
                                        <div class="row align-items-end">
                                            <div class="col-md-6 form-group">
                                                <label>Ethnicity*</label>                                                                
                                                <select id='ethnicity' name='ethnicity' class="w-100 field">
                                                    <option value="Select ethnicity">Select ethnicity</option>
                                                    <option value="Select ethnicity">Select ethnicity</option>
                                                </select>
                                            </div>
                                            <div class="col-md-6 form-group">
                                                <label>NHS number (if known)</label>                                                                
